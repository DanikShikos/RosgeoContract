@model IEnumerable<RosGeoDevOps.Auth>

@{
    ViewBag.Title = "Index";
}

<h2>Список</h2>

<p>
    @Html.ActionLink("Добавить запись", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Login)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sotrudnik.Fam)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Role.Naim)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Login)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sotrudnik.Fam)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Role.Naim)
        </td>
        <td>
            @Html.ActionLink("Изменить", "Edit", new { id=item.ID_Auth }) |
            @Html.ActionLink("Подробности", "Details", new { id=item.ID_Auth }) |
            @Html.ActionLink("Удалить", "Delete", new { id=item.ID_Auth })
        </td>
    </tr>
}

</table>
